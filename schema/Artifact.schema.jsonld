{
  "@context": "$AMORA/Schema.jsonld",
  "@graph": [
    {
      "@id": "$ID/",
      "amora:schemaId": [
        "$ID/name",
        "$ID/organization",
        "$ID/version"
      ],
      "amora:schemaAuthor": "amora",
      "amora:schemaVersion": "0.1",
      "@type": "http://amora.center/kb/amora/Schema/0.1/"
    },
    {
      "@id": "$ID/name",
      "amora:schemaProperty": "name",
      "amora:schemaType": "schema:name"
    },
    {
      "@id": "$ID/organization",
      "amora:schemaProperty": "organization",
      "amora:schemaType": "schema:Text"
    },
    {
      "@id": "$ID/version",
      "amora:schemaProperty": "version",
      "amora:schemaType": "schema:Text"
    }
  ]
}

