    {
      "@id": "$ID/",
      "amora:schemaId": [
        "amora:Artifact/name",
        "amora:Artifact/organization",
        "amora:Artifact/version"
      ],
      "amora:schemaAuthor": "amora",
      "amora:schemaVersion": "0.1"
    },
    {
      "@id": "$ID/name",
